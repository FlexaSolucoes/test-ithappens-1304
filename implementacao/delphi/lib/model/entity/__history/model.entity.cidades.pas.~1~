  type Tcidades = class
  private
    { Private declarations }
    Fcidade_id          : Integer;
    Fcidade_descricao   : String ;
    Fcidade_cpf         : String ;
    Fcidade_email       : String ;
    Fcidade_senha       : String ;
  public
