unit Model.Patterns.types;

interface

    type TDatabaseType = (
                          tdbMSSQL   ,
                          tdbOracle  ,
                          tdbSQLLite ,
                          tdbMySQL   ,
                          tdbPostgree,
                          tdbFirebird);

    type TMensagemTipo = (
                          tmtNone    ,
                          tmtEnviada ,
                          tmtRecebida);

    type TRetorno = record
      retorno_id       : integer;
      retorno_cod_erro : integer;
      retorno_msg_erro : string ;
    end;


implementation

end.
